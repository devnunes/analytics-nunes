{
  "short_name": "Analytics Watcher",
  "name": "AW",
  "version": "0.1",
  "manifest_version": 3,
  "devtools_page": "devtools.html",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "./static/js/content.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "storage",
    "webRequest",
    "activeTab"
  ]
}